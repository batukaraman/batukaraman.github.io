{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"]
    },
    "esModuleInterop": true,
    "skipLibCheck": true,
    "moduleResolution": "node",
    "jsx": "react-jsx",
    "lib": ["es2015", "dom"],
    "module": "ESNext",
    "target": "ES6",
    "outDir": "./dist"
  },
  "include": ["src", "vite-env.d.ts"]
}
